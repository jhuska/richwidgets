<script>
  var list_options = {
    header: "List layout" //caption
  };
  var list_dragselect_options = {
    header: "List layout w/ drag-select", //caption
    dragSelect: true
  }
  var table_options = {
    header: "Table layout"
  };
  var table_dragselect_options = {
    header: "Table layout w/ drag-select",
    dragSelect: true
  };
  $("#list").orderingList(list_options);
  $("#list_dragselect").orderingList(list_dragselect_options);
  $("#table").orderingList(table_options);
  $("#table_dragselect").orderingList(table_dragselect_options);

</script>
